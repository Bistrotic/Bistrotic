                    <xs:restriction
                      base="xs:string">
                      <xs:minLength
                        value="1"/>
                      <xs:maxLength
                        value="20"/>
                      <xs:whiteSpace
                        value="collapse"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
                <xs:attribute
                  name="NIV"
                  use="optional">
                  <xs:annotation>
                    <xs:documentation>Atributo condicional para expresar el número de serie o número de identificación vehicular de la carrocería del vehículo, si el AñoModelo es posterior o igual a 1980.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction
                      base="xs:string">
                      <xs:length
                        value="17"/>
                      <xs:whiteSpace
                        value="collapse"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
