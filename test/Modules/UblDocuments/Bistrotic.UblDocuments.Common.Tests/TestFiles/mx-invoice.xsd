<?xml version="1.0" encoding="UTF-8"?>
<!-- cambia, git no te ve -->
<xs:schema
  attributeFormDefault="unqualified"
  elementFormDefault="qualified"
  targetNamespace="http://www.fact.com.mx/schema/fx"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:fx="http://www.fact.com.mx/schema/fx">
  <xs:element
    name="FactDocsMX">
    <xs:complexType>
      <xs:sequence>
        <xs:element
          maxOccurs="unbounded"
          name="FactDocMX"
          type="fx:TFactDocMX"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element
    name="FactDocMX"
    type="fx:TFactDocMX"/>
  <xs:complexType
    name="TFactDocMX">
    <xs:sequence>
      <xs:element
        fixed="7"
        name="Version"
        type="xs:integer"/>
      <xs:element
        name="Identificacion">
        <xs:complexType>
          <xs:sequence>
            <xs:element
              fixed="MX"
              name="CdgPaisEmisor"
              type="fx:TSenderCountryCode"> </xs:element>
            <xs:element
              name="TipoDeComprobante"
              type="fx:TTipoDeDocumento"
              minOccurs="0"> </xs:element>
            <xs:element
              name="RFCEmisor">
              <xs:simpleType>
                <xs:restriction
                  base="fx:TRFC">
                  <xs:pattern
                    value="[A-ZÑ&amp;]{3}[A-ZÑ]{0,1}[0-9]{2}(0[1-9]|1[012])(0[1-9]|[12][0-9]|3[01])[A-Z0-9]{2}[A0-9]{1}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="RazonSocialEmisor">
              <xs:simpleType>
                <xs:restriction
                  base="fx:TCadenaPermitida">
                  <xs:maxLength
                    value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              name="Usuario">
              <xs:simpleType>
                <xs:restriction
                  base="fx:TCadenaPermitida">
                  <xs:maxLength
                    value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="NumeroInterno">
              <xs:annotation>
                <xs:documentation>Puede informarse otro identificador no fiscal.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction
                  base="fx:TCadenaPermitida">
                  <xs:maxLength
                    value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="AsignacionSolicitada">
              <xs:annotation>
                <xs:documentation>Solo cfdv3 (cfdi).</xs:documentation>
                <xs:documentation>Si existe, el motor tratará realizar la asignación.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element
                    minOccurs="0"
                    name="Serie">
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:minLength
                          value="1"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    minOccurs="0"
                    name="Folio">
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:minLength
                          value="1"/>
                        <xs:maxLength
                          value="40"/>
                        <xs:pattern
                          value="[^|]{1,40}"/>
                        <xs:whiteSpace
                          value="collapse"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    minOccurs="0"
                    name="TiempoDeEmision"
                    type="xs:dateTime">
                    <xs:annotation>
                      <xs:documentation>Si no existe, el motor asignará Now()</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element
              name="LugarExpedicion">
              <xs:simpleType>
                <xs:restriction
                  base="xs:string">
                  <xs:minLength
                    value="1"/>
                  <xs:maxLength
                    value="2048"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="NumCtaPago">
              <xs:simpleType>
                <xs:restriction
                  base="xs:string">
                  <xs:minLength
                    value="4"/>
                  <xs:maxLength
                    value="256"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="TotalDeParcialidades">
              <xs:complexType>
                <xs:sequence>
                  <xs:choice>
                    <xs:element
                      name="FolioFiscalOrigUuid"
                      type="fx:TGuid"/>
                    <xs:element
                      name="FolioFiscalOrigNum">
                      <xs:simpleType>
                        <xs:restriction
                          base="xs:positiveInteger"/>
                      </xs:simpleType>
                    </xs:element>
                  </xs:choice>
                  <xs:element
                    minOccurs="0"
                    name="SerieFolioFiscalOrig">
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:minLength
                          value="1"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    name="FechaFolioFiscalOrig"
                    type="xs:dateTime"/>
                  <xs:element
                    name="MontoFolioFiscalOrig"
                    type="fx:TNonNegativeAmount"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="Confirmacion">
              <xs:simpleType>
                <xs:restriction
                  base="xs:string">
                  <xs:minLength
                    value="1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="CadenaOriginal">
              <xs:annotation>
                <xs:documentation>No emplear este elemento.</xs:documentation>
                <xs:documentation>Será incluido automáticamente.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction
                  base="xs:string">
                  <xs:minLength
                    value="1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element
        minOccurs="0"
        name="Cancelaciones">
        <xs:complexType>
          <xs:sequence>
            <xs:element
              maxOccurs="100"
              name="CancelaYSustituye">
              <xs:complexType>
                <xs:sequence>
                  <xs:element
                    name="Numero"
                    type="fx:TCode"/>
                  <xs:element
                    minOccurs="0"
                    name="Fecha"
                    type="xs:date"/>
                  <xs:element
                    minOccurs="0"
                    name="TipoCancelacion">
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:minLength
                          value="1"/>
                        <xs:maxLength
                          value="10"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    minOccurs="0"
                    name="ConceptoCancelacion">
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:minLength
                          value="1"/>
                        <xs:maxLength
                          value="20"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element
        minOccurs="0"
        name="CfdiRelacionados">
        <xs:complexType>
          <xs:sequence>
            <xs:element
              name="TipoRelacion"
              type="xs:string"/>
            <xs:element
              maxOccurs="unbounded"
              name="UUID">
              <xs:simpleType>
                <xs:restriction
                  base="xs:string">
                  <xs:length
                    value="36"/>
                  <xs:pattern
                    value="[a-f0-9A-F]{8}-[a-f0-9A-F]{4}-[a-f0-9A-F]{4}-[a-f0-9A-F]{4}-[a-f0-9A-F]{12}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element
        minOccurs="0"
        name="Origen">
        <xs:complexType>
          <xs:sequence>
            <xs:element
              minOccurs="0"
              name="Area">
              <xs:annotation>
                <xs:documentation>TWEB, FIA, SUN, etc.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction
                  base="xs:string">
                  <xs:minLength
                    value="1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="Modulo">
              <xs:annotation>
                <xs:documentation>DOMICILIADOS, SUBSECUENTES, etc.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction
                  base="xs:string">
                  <xs:minLength
                    value="1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element
        minOccurs="0"
        name="Procesamiento"
        type="fx:TDictionaries"> </xs:element>
      <xs:element
        name="Emisor">
        <xs:complexType>
          <xs:sequence>
            <xs:element
              minOccurs="0"
              name="DomicilioFiscal"
              type="fx:TDomicilioMexicano"/>
            <xs:element
              minOccurs="0"
              name="DomicilioDeEmision"
              type="fx:TDomicilioMexicano"/>
            <xs:element
              name="RegimenFiscal">
              <xs:complexType>
                <xs:sequence>
                  <xs:element
                    maxOccurs="1"
                    name="Regimen">
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:minLength
                          value="1"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element
        name="Receptor">
        <xs:complexType>
          <xs:sequence>
            <xs:element
              name="CdgPaisReceptor"
              type="fx:TCountryCode"/>
            <xs:element
              name="RFCReceptor"
              type="fx:TRFC"/>
            <xs:element
              minOccurs="0"
              name="TaxID"
              type="fx:TCadenaPermitida">
              <xs:annotation>
                <xs:documentation>Para no mexicanos.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="NombreReceptor">
              <xs:annotation>
                <xs:documentation>Si el emisor es mexicano, se entiende como Razón Social.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction
                  base="fx:TCadenaPermitida">
                  <xs:maxLength
                    value="250"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="Domicilio">
              <xs:complexType>
                <xs:choice>
                  <xs:element
                    name="DomicilioFiscalMexicano"
                    type="fx:TDomicilioMexicano"/>
                  <xs:element
                    name="OtroDomicilio"
                    type="fx:TDomicilio"/>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="DomicilioDeRecepcion">
              <xs:complexType>
                <xs:choice>
                  <xs:element
                    name="DomicilioFiscalMexicano"
                    type="fx:TDomicilioMexicano"/>
                  <xs:element
                    name="OtroDomicilio"
                    type="fx:TDomicilio"/>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element
              minOccurs="0"
              name="ResidenciaFiscal"
              type="fx:c_Pais"> </xs:element>
            <xs:element
              minOccurs="0"
              name="UsoCFDI">
              <xs:simpleType>
                <xs:restriction
                  base="xs:string">
                  <xs:minLength
                    value="1"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element
        name="Conceptos">
        <xs:complexType>
          <xs:sequence>
            <xs:element
              maxOccurs="unbounded"
              name="Concepto">
              <xs:complexType>
                <xs:sequence>
                  <xs:element
                    name="Cantidad"
                    type="fx:TQuantity">
                    <xs:annotation>
                      <xs:documentation>Cantidad</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element
                    name="ClaveUnidad">
                    <xs:annotation>
                      <xs:documentation>NUEVO CFDI 3.3 REQUERIDO</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:minLength
                          value="1"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    name="UnidadDeMedida"
                    minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Unidad</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:minLength
                          value="1"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    minOccurs="1"
                    name="ClaveProdServ">
                    <xs:annotation>
                      <xs:documentation>NUEVO CFDI 3.3 REQUERIDO</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:maxLength
                          value="32"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    minOccurs="0"
                    name="Codigo">
                    <xs:annotation>
                      <xs:documentation>Codigo</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction
                        base="xs:string">
                        <xs:maxLength
                          value="100"/>
                        <xs:minLength
                          value="1"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    name="Descripcion">
                    <xs:annotation>
                      <xs:documentation>Descripcion</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction
                        base="fx:TCadenaPermitida">
                        <xs:pattern
                          value="[^|]*"/>
                        <xs:maxLength
                          value="1000"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element
                    name="ValorUnitario"
                    type="fx:TNonNegativeAmount">
                    <xs:annotation>
                      <xs:documentation>ValorUnitario</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element
                    name="Importe"
                    type="fx:TNonNegativeAmount">
                    <xs:annotation>
                      <xs:documentation>Importe
</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element
                    minOccurs="0"
                    name="Descuento"
                    type="fx:TNonNegativeAmount"> </xs:element>
                  <xs:element
                    minOccurs="0"
                    name="ImpuestosSAT">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element
                          minOccurs="0"
                          name="Traslados">
                          <xs:annotation>
                            <xs:documentation>Nodo opcional para asentar los impuestos trasladados aplicables al presente concepto.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element
                                maxOccurs="unbounded"
                                name="Traslado">
                                <xs:annotation>
                                  <xs:documentation>Nodo requerido para asentar la información detallada de un traslado de impuestos aplicable al presente concepto.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                  <xs:attribute
                                    name="Base"
                                    type="fx:t_Importe"
                                    use="required">
                                    <xs:annotation>
                                      <xs:documentation>Atributo requerido para señalar la base para el cálculo del impuesto, la determinación de la base se realiza de acuerdo con las disposiciones fiscales vigentes. No se permiten valores negativos.</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                  <xs:attribute
                                    name="Impuesto"
                                    type="fx:c_Impuesto"
                                    use="required">
                                    <xs:annotation>
                                      <xs:documentation>Atributo requerido para señalar la clave del tipo de impuesto trasladado aplicable al concepto.</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                  <xs:attribute
                                    name="TipoFactor"
                                    type="fx:c_TipoFactor"
                                    use="required">
                                    <xs:annotation>
                                      <xs:documentation>Atributo requerido para señalar la clave del tipo de factor que se aplica a la base del impuesto.</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                  <xs:attribute
                                    name="TasaOCuota"
                                    use="optional">
                                    <xs:annotation>
                                      <xs:documentation>Atributo condicional para señalar el valor de la tasa o cuota del impuesto que se traslada para el presente concepto. Es requerido cuando el atributo TipoFactor tenga una clave que corresponda a Tasa o Cuota.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                      <xs:restriction
                                        base="xs:decimal">
                                        <xs:minInclusive
                                          value="0.000000"/>
                                        <xs:fractionDigits
                                          value="6"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:attribute>
                                  <xs:attribute
                                    name="Importe"
                                    type="fx:t_Importe33"
                                    use="optional">
                                    <xs:annotation>
                                      <xs:documentation>Atributo condicional para señalar el importe del impuesto trasladado que aplica al concepto. No se permiten valores negativos. Es requerido cuando TipoFactor sea Tasa o Cuota</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                </xs:complexType>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                        <xs:element
                          minOccurs="0"
                          name="Retenciones">
                          <xs:annotation>
                            <xs:documentation>Nodo opcional para asentar los impuestos retenidos aplicables al presente concepto.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element
                                maxOccurs="unbounded"
                                name="Retencion">
                                <xs:annotation>
                                  <xs:documentation>Nodo requerido para asentar la información detallada de una retención de impuestos aplicable al presente concepto.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                  <xs:attribute
                                    name="Base"
                                    type="fx:t_Importe33"
                                    use="required">
                                    <xs:annotation>
                                      <xs:documentation>Atributo requerido para señalar la base para el cálculo de la retención, la determinación de la base se realiza de acuerdo con las disposiciones fiscales vigentes. No se permiten valores negativos.</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                  <xs:attribute
                                    name="Impuesto"
                                    type="fx:c_Impuesto"
                                    use="required">
                                    <xs:annotation>
                                      <xs:documentation>Atributo requerido para señalar la clave del tipo de impuesto retenido aplicable al concepto.</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                  <xs:attribute
                                    name="TipoFactor"
                                    type="fx:c_TipoFactor"
                                    use="required">
                                    <xs:annotation>
                                      <xs:documentation>Atributo requerido para señalar la clave del tipo de factor que se aplica a la base del impuesto.</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                  <xs:attribute
                                    name="TasaOCuota"
                                    use="required">
                                    <xs:annotation>
                                      <xs:documentation>Atributo requerido para señalar la tasa o cuota del impuesto que se retiene para el presente concepto.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                      <xs:restriction
                                        base="xs:decimal">
                                        <xs:minInclusive
                                          value="0.000000"/>
                                        <xs:fractionDigits
                                          value="6"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:attribute>
                                  <xs:attribute
                                    name="Importe"
                                    type="fx:t_Importe33"
                                    use="required">
                                    <xs:annotation>
                                      <xs:documentation>Atributo requerido para señalar el importe del impuesto retenido que aplica al concepto. No se permiten valores negativos.</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                </xs:complexType>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element
                    minOccurs="0"
                    name="Opciones">
                    <xs:complexType>
                      <xs:sequence
                        minOccurs="0">
                        <xs:element
                          minOccurs="0"
                          name="DatosDeImportacion"
                          maxOccurs="1">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element
                                maxOccurs="unbounded"
                                name="InformacionAduanera"
                                type="fx:TInformacionAduanera"/>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                        <xs:element
                          minOccurs="0"
                          name="CuentaPredial">
                          <xs:simpleType>
                            <xs:restriction
                              base="xs:string">
                              <xs:minLength
                                value="1"/>
                              <xs:maxLength
                                value="256"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element
                          minOccurs="0"
                          name="ComplementoConcepto">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element
                                minOccurs="0"
                                name="InstEducativas">
                                <xs:complexType>
                                  <xs:sequence>
                                    <xs:element
                                      name="NombreAlumno">
                                      <xs:simpleType>
                                        <xs:restriction
                                          base="xs:string">
                                          <xs:minLength
                                            value="1"/>
                                          <xs:maxLength
                                            value="1024"/>
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:element>
                                    <xs:element
                                      name="CURP"
                                      type="fx:TCURP"/>
                                    <xs:element
                                      name="NivelEducativo">
                                      <xs:simpleType>
                                        <xs:restriction
                                          base="xs:string">
                                          <xs:enumeration
                                            value="Preescolar"/>
                                          <xs:enumeration
                                            value="Primaria"/>
                                          <xs:enumeration
                                            value="Secundaria"/>
                                          <xs:enumeration
                                            value="Profesional técnico"/>
                                          <xs:enumeration
                                            value="Bachillerato o su equivalente"/>
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:element>
                                    <xs:element
                                      name="AutRVOE">
                                      <xs:simpleType>
                                        <xs:restriction
                                          base="xs:string">
                                          <xs:minLength
                                            value="1"/>
                                          <xs:maxLength
                                            value="1024"/>
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:element>
                                    <xs:element
                                      minOccurs="0"
                                      name="RfcPago"
                                      type="fx:TRFC"/>
                                  </xs:sequence>
                                </xs:complexType>
                              </xs:element>
                              <xs:element
                                minOccurs="0"
                                name="VentaVehiculos11">
                                <xs:annotation>
