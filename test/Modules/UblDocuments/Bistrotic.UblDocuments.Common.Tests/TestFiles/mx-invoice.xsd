        name="To"
        type="fx:TEmail"/>
      <xs:element
        maxOccurs="10"
        minOccurs="0"
        name="Cc"
        type="fx:TEmail"/>
      <xs:element
        maxOccurs="10"
        minOccurs="0"
        name="Bcc"
        type="fx:TEmail"/>
